{
  "name": "kery-map-eslint",
  "version": "1.0.0",
  "description": "eslint config for kery-map",
  "main": "index.js",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "repository": "https://github.com/kery-map/kery-map-eslint.git",
  "author": "DZG-MELODY <dongzhigang531@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn && lerna bootstrap",
    "clean": "lerna clean",
    "publish:prepare": "lerna publish --conventional-commits --conventional-prerelease --access public",
    "publish:release": "lerna publish --conventional-commits --conventional-graduate --access public",
    "publish:bump": "lerna publish from-git --conventional-commits --access public"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "lerna": "^3.22.1"
  },
  "peerDependencies": {
    "eslint": "^7.5.0"
  }
}
